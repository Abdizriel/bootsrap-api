{
  "name": "bootstrap-api",
  "version": "1.0.0",
  "description": "Bootstrap API",
  "author": "Marcin Mrotek",
  "license": "UNLICENSED",
  "engines": {
    "node": ">= 12",
    "npm": ">= 6"
  },
  "main": "dist/handler.js",
  "scripts": {
    "build": "tsc",
    "watch-build": "tsc -w",
    "watch-server": "nodemon -w dist/ -x 'node dist/index'",
    "start": "node dist/index",
    "dev": "concurrently -k -p '[{name}]' -n 'TypeScript,Service' -c 'yellow.bold,cyan.bold,green.bold' 'npm run watch-build' 'npm run watch-server'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migration:run": "ts-node node_modules/.bin/typeorm migration:run",
    "migration:revert": "ts-node node_modules/.bin/typeorm migration:revert",
    "format": "prettier --write src/**/*.{ts,tsx,scss,css,json}"
  },
  "keywords": [],
  "dependencies": {
    "@graphql-tools/load-files": "^6.2.4",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "apollo-server-express": "^2.10.1",
    "apollo-server-lambda": "^2.10.0",
    "axios": "^0.20.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cron": "^1.8.2",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "generate-password": "^1.5.1",
    "graphql": "^15.0.0",
    "graphql-relay": "^0.6.0",
    "graphql-upload": "^11.0.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.31",
    "pg": "^8.0.3",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.6",
    "slugify": "^1.4.5",
    "subscriptions-transport-ws": "^0.9.16",
    "tslib": "^2.0.0",
    "type-graphql": "^1.0.0-rc.1",
    "typedi": "^0.8.0",
    "typegoose": "^5.9.1",
    "typeorm": "^0.2.28",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^4.0.3",
    "uuidv4": "^6.0.7",
    "winston": "^3.2.1",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.150",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^14.0.0",
    "@types/randomstring": "^1.1.6",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0"
  },
  "_moduleAliases": {
    "@entities": "dist/entities",
    "@providers": "dist/providers",
    "@domains": "dist/domains",
    "@repositories": "dist/repositories",
    "@internalTypes": "dist/internalTypes",
    "@config": "dist/config"
  }
}
